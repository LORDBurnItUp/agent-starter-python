# LiveKit Configuration (Required)
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Supabase Configuration (Optional - for data persistence)
# Sign up for free at https://supabase.com
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_KEY=

# RAG System Configuration (Optional - for self-improvement features)
# Enable/disable RAG-based self-improvement system (default: true)
ENABLE_RAG=true

# Auto-generate improvement reports every N conversations (default: 100)
RAG_IMPROVEMENT_INTERVAL=100

# Database paths (defaults shown - customize if needed)
# RAG_DB_PATH=data/conversations.db
# RAG_CHROMA_PATH=data/chroma_db
